<% if @updated %>
  $('.js-examinationResults-modal-item[data-id="<%= @examination_result.id%>"]')
    .html("<%= j render('list_item', applicant: @applicant, result: @examination_result) %>")
<% else %>
  $('.js-examinationResults-modal-item[data-id="<%= @examination_result.id%>"]')
    .html("<%= j render('form', examination_result: @examination_result,
          applicant: @applicant,
          url: manage_applicant_examination_result_path(@applicant, @examination_result),
          from_edit_page: true,
          method: :put) %>")
<% end %>

$('#js-flash_notifications').html("<%= j render_flash_notifications %>")
