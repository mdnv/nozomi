<% if @updated %>
  $('.js-studyingResults-modal-item[data-id="<%= @studying_result.id%>"]')
    .html("<%= j render('list_item', applicant: @applicant, result: @studying_result) %>")
<% else %>
  $('.js-studyingResults-modal-item[data-id="<%= @studying_result.id%>"]')
    .html("<%= j render('form', studying_result: @studying_result,
          applicant: @applicant,
          url: manage_applicant_studying_result_path(@applicant, @studying_result),
          from_edit_page: true,
          saveable: @studying_result.savable?,
          method: :put) %>")

  $('.js-delayed-datepicker').datepicker({ format: "<%= t('formats.date_picker') %>", autoclose: true, clearBtn: true })
<% end %>

$('#js-flash_notifications').html("<%= j render_flash_notifications %>")
